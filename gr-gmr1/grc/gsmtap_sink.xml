<?xml version="1.0"?>
<block>
 <name>GSMTap Sink</name>
 <key>gsmtap_sink</key>
 <category>GMR-1</category>
 <import>from gnuradio import gmr1</import>
 <make>gmr1.gsmtap_sink($host, $port)</make>
 <param>
   <name>Host</name>
   <key>host</key>
   <value>127.0.0.1</value>
   <type>string</type>
 </param>
 <param>
   <name>Port</name>
   <key>port</key>
   <value>4729</value>
   <type>int</type>
 </param>
 <sink>
   <name>pdus</name>
   <type>message</type>
 </sink>
</block>
